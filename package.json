{
  "name": "oexar",
  "version": "0.1.0",
  "description": "D&D 5th Edition (2024) character management assistant",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "dnd",
    "d&d",
    "5e",
    "2024",
    "character-sheet",
    "rpg"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "eslint": "^9.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^2.0.0"
  }
}
